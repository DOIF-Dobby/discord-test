{
  "compilerOptions": {
    "target": "ES2021",
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "discord.d.ts"],
  "exclude": ["node_modules", "dist"],
  "$schema": "https://json.schemastore.org/tsconfig"
}
